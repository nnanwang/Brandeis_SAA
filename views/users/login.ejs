<div class="data-form">
<form action="/users/login" method="POST">
    <h1>Login</h1>
    <div class="form-row">
    <label for="inputEmail">Email address</label>
    <input
      type="email"
      name="email"
      id="inputEmail"
      placeholder="Email address"
      required
    />
    </div>
    <div class="form-row">
    <label for="inputPassword">Password</label>
    <input
      type="password"
      name="password"
      id="inputPassword"
      placeholder="Password"
      required
    />
    </div>
    <button type="submit">Login</button>
  </form>
</div>